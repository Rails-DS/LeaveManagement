
<h2>Teams </h2>

<div class="container">
		<table class="table table-borderless mt-5">
		  <tr class="table-primary">
		    <th>Name</th>
		    <th>Team Leader</th>
		  </tr>
		<% @teams.each do |team| %>
		  <tr>
		    <td><%= team.teamName %></td>
		    <td><%= team.teamLeader %></td>
		    <td><%= button_to 'Edit', edit_team_path(team.id), { class: "btn btn-light button" , method: :get }%></td>
		    <!-- <td><%#= button_to 'Delete', team_path(team.id), method: :delete%></td> -->
		  </tr>
		<%end%>
 </div>


<%= link_to 'New Team', new_team_path, { class: "btn btn-primary" }%>
<%= link_to "Back to home", users_path, {class: "btn btn-primary"} %>




